: objects_init
	va := screen_6_7_teleport_2
	teleport_activate
	return

: objects_tick
	map_get_screen_index
	jump0 map_tick_objects

: objects_draw
	map_get_screen_index
	jump0 map_draw_objects

: objects_screen_entered
	jump objects_draw

: objects_collide
	map_get_screen_index
	i := ninja_current_pos
	load va - vb
	jump0 map_collide_objects
